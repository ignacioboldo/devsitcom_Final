@model IEnumerable<BussinesEntities.HabitacionesDisponiblesEntities>


@{
    if (Model.Count() > 0)
    {

 
        foreach (var item in Model)
        {
            <option value="@item.Id">@item.Nombre</option>
        }



    }
    else
    {
        <option value="">Sin Datos</option>

    }

}
    




